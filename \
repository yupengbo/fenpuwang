# -*- coding: utf-8 -*-

from django.conf.urls import patterns, url

from apps.activities import views

urlpatterns = patterns('',
  url(r'^(?P<activity_id>\d+)/$', views.activity, name='acitivity'),
  url(r'^(?P<activity_id>\d+)/info$', views.activity, name='acitivity'),
  url(r'^wx_auth/$', views.wx_auth, name='wx_auth'),
)

